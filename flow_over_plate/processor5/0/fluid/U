FoamFile
{
    format      binary;
    class       volVectorField;
    location    "0/fluid";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.1 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform List<vector> 
0
;
    }
    outlet
    {
        type            pressureInletOutletVelocity;
        value           nonuniform List<vector> 
0
;
    }
    top
    {
        type            slip;
    }
    fluid_slip-bottom
    {
        type            slip;
    }
    fluid_no_slip-bottom
    {
        type            noSlip;
    }
    front_back
    {
        type            empty;
    }
    fluid_to_solid
    {
        type            noSlip;
    }
    procBoundary5to3
    {
        type            processor;
        value           uniform (0.1 0 0);
    }
    procBoundary5to4
    {
        type            processor;
        value           uniform (0.1 0 0);
    }
}


