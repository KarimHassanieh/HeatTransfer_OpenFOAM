FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0/B";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    left
    {
        type            zeroGradient;
    }
    bottom
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
    B_to_C
    {
        type            calculated;
        value           uniform 0;
    }
    B_to_A
    {
        type            calculated;
        value           uniform 0;
    }
}


