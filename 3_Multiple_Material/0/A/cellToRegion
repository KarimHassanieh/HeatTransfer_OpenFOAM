FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0/A";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 2;

boundaryField
{
    left
    {
        type            zeroGradient;
    }
    right
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
    A_to_C
    {
        type            calculated;
        value           uniform 0;
    }
    A_to_B
    {
        type            calculated;
        value           uniform 0;
    }
}


// ************************************************************************* //
