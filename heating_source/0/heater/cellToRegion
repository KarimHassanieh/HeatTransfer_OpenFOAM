FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0/heater";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 2;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    heater_to_metal
    {
        type            calculated;
        value           uniform 0;
    }
}


