FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "1/metal";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    metal_to_fluid
    {
        type            calculated;
        value           uniform 0;
    }
    metal_to_heater
    {
        type            calculated;
        value           uniform 0;
    }
}


